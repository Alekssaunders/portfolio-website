---
import Layout from './Layout.astro';
import NavBar from '../components/NavBar/NavBar.astro';
import "./ResponsivePage.css";

interface Props {
    title: string;
}

const {title} = Astro.props;
---

<Layout title={title}>
    <div class="responsivePagePageContainer">
        <div class="responsivePageNavContainer">
            <NavBar/>
        </div>
        <main class="responsivePageMain">
            <slot/>
        </main>
    </div>
</Layout>

